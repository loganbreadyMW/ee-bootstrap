{
	"name": "project-name",
	"host": "host-name",
	"protocol": "sftp",
	"port": 22,
	"username": "username",
	"password": "password",
	"remotePath": "/remote-path",
	"uploadOnSave": false,
	"syncOption": {
		"update": true,
		"delete": true
	},
	"ignore": [
		".DS_Store",
		".editorconfig",
		".git",
		".git*",
		".prettierignore",
		".prettierrc",
		".stylelintignore",
		".stylelintrc",
		".vscode",
		"/**/.DS_Store",
		"/**/Thumbs.db",
		"gulpfile.js",
		"init.sh",
		"node_modules",
		"package-lock.json",
		"package.json",
		"sftp-config.json",
		"sftp.json",
		"Thumbs.db"
	],
	"connectionTimeout": 30
}
